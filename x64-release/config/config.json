{
    "logging": {
        "name": "app",
        "level": "debug",
        "path": "./logs",
        "rotate": {
            "max_size_mb": 10,
            "max_files": 5
        },
        "format": "%Y-%m-%dT%H:%M:%S.%fZ %l [%P:%t] %n : %v",
        "offline_events_file": "./logs/offline_events.log"
    },
    "xfs": {
        "app_id": "XFS_CLIENT_APP",
        "timeout_ms": 30000,
        "services": [
            "CDM1",  
            "PTR1",
            "IDC1",
            "PIN1"
        ],
        "event_classes": [
            "SERVICE_EVENTS",
            "USER_EVENTS",
            "SYSTEM_EVENTS",
            "EXECUTE_EVENTS" 
        ]
    }
}